{
    "name": "kdrmlhcn/nova-multiselect-label-filter",
    "description": "Multiselect filter for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "multiselect",
        "label",
        "filter"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.8 || ^6.0 || ^7.0",
        "laravel/nova": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kdrmlhcn\\NovaMultiselectFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kdrmlhcn\\NovaMultiselectFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
